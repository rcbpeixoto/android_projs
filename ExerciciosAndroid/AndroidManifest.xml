<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.fbv.rcbop"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="9" />

    <application android:icon="@drawable/icon" android:label="@string/app_name">
        <activity android:name=".ui.MenuActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.exe1.Exe1Activity" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity android:name=".ui.exe2.Exe2Activity" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity android:name=".ui.exe3.Exe3Activity" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity android:name=".ui.exe4.Exe4Activity" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity android:name=".ui.exe5.Exe5Activity" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <activity android:name=".ui.exe2.ActivityIntent" android:theme="@android:style/Theme.NoTitleBar"></activity>
        <receiver android:name=".ui.exe5.Exe5BCR">
        	<intent-filter>
        		<action android:name="ROGER_BROADCAST"/>
        		<category android:name="android.intent.category.DEFAULT"/>
        	</intent-filter>
        </receiver>
    </application>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
</manifest>